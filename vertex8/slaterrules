%nconflict_d(I,O);nconflict_d(I,P);nconflict_d(I,Q);nconflict_d(I,R);ver_order_d(I,J) :- ver(I),ver(J),ver(O),ver(P),ver(Q),ver(R),O!=P,P!=Q,Q!=R,O!=Q,O!=R,P!=R,O!=J,P!=J,Q!=J,R!=J.
